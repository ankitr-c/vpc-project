docker run -d \
-p 27017:27017 \
-e MONGO_INITDB_ROOT_USERNAME:root \
-e MONGO_INITDB_ROOT_PASSWORD:root \
--name mongodb \
--net mongo-network \
mongo

docker run -d \
-p 8081:8081 \
-e ME_CONFIG_MONGODB_SERVER="mongodb" \
-e ME_CONFIG_BASICAUTH_USERNAME="root" \
-e ME_CONFIG_BASICAUTH_PASSWORD="root" \
--name mongo-express \
--net mongo-network \
mongo-express

